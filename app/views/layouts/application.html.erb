<!DOCTYPE html>
<html>
<head>
  <title>LoginApp</title>
     <%= render 'layouts/stylesheets' %>
</head>
<body>
<div id="container">
<%= render 'layouts/header' %>

<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>
<%= yield %>
<%= debug(params) if Rails.env.development? %>
</div>
</body>
</html>
